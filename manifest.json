{
	"name": "Zipply+",
	"version": "0.6",
    "manifest_version": "2",
	
	"app": {
    	"launch": {
      		"local_path": "popup.html"
		}
    },

    "description": "Love music even more.",
    
    "icons": {
		"16": "img/icon_16.png",
		"128": "img/icon_128.png",
		"256": "img/icon_256.png"
	},
	
	"permissions": [
		"https://www.google.com/*", "http://*/*", "https://*/*", "tabs", "http://*.zippyshare.com/*"
	]
}